<%-include("./partials/header.ejs")%>
    <div class="register-main">
    </div>
    <form action="/register" method="POST">
        <div class="form-container container-fluid shadow p-5">
            <h1 class="text-center pb-3">Create Account</h1>
            <input type="text" placeholder="First Name" class="form-control p-3 mb-3">
            <input type="text" placeholder="Last Name" class="form-control p-3 mb-3">
            <input type="email" placeholder="jhondoe@gmail.com" class="form-control p-3 mb-3">
            <input type="password" placeholder="Password" class="form-control p-3 mb-3">
            <input type="text" placeholder="T.C Kimlik" class="form-control p-3 mb-3">
            <h5 class="p-2 mb-3">Upload Image:</h5> 
            <input type="file" class="p-3 mb-3">
            <div class="text-center">
                <button type="submit" class="btn btn-primary">Register</button>
            </div>
        </div>
    </form>
